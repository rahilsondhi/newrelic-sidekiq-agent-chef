newrelic:
  license_key: '<%= node['newrelic_sidekiq_agent']['newrelic_license_key'] %>'
  #
  # Set to '1' for verbose output, remove for normal output.
  # All output goes to stdout/stderr.
  #
  #verbose: 1
#
# Agent Configuration:
#
agents:
  sidekiq_status_agent:
<%- node['newrelic_sidekiq_agent']['instances'].each do |instance| %>
    -
      instance_name: "<%= instance['instance_name'] %>"
      uri: "<%= instance['uri'] %>"
      namespace: "<%= instance['namespace'] %>"
<%- end %>
